02B2 C292              409             CLR E
                       410
02B4 A2E3              411             MOV C, ACC.3
02B6 9297              412             MOV DB7, C
02B8 A2E2              413             MOV C, ACC.2
02BA 9296              414             MOV DB6, C
02BC A2E1              415             MOV C, ACC.1
02BE 9295              416             MOV DB5, C
02C0 A2E0              417             MOV C, ACC.0
02C2 9294              418             MOV DB4, C
