                         4     ;------SERVO MOTOR(PWM) e LCD --------
  0093                   5             RS  EQU P1.3
  0092                   6             E   EQU P1.2
  0097                   7             DB7 EQU P1.7
  0096                   8             DB6 EQU P1.6
  0095                   9             DB5 EQU P1.5
  0094                  10             DB4 EQU P1.4
                        11
  00A0                  12             Pulso   equ     p2.0    ;saida para o pulso
  00B2                  13             Echo    equ     p3.2
  00A7                  14             Trig    equ     p2.7
                        15     ;-----------Reset-----------
                        16             org     000h
0000 020003             17             jmp     inicio
                        18     inicio:
0003 120259             19             call    lcd_init
0006 75A000             20             mov     P2,#000h        ;define como saida de dados
0009 C2B2               21             clr     echo
000B D28C               22             setb    tr0             ;
000D 758909             23             mov     tmod,#09h       ; configura o contador pra contar enquanto o int  for a um 1 (timer 1
0010 7F01               24             mov     r7,#1h
0012 7E01               25             mov     r6,#1h
0014 7D01               26             mov     r5,#1h
0016 7C01               27             mov     r4,#1h
0018 7B12               28             mov     r3,#12h
001A 02001D             29             jmp     PulsoPWM
                        30
                        31
                        32     ;--------rotinas pwm ----------- (pulso de 50Hz confome o controle do servo controle do servo)
                        33     ;tempo de Ms (IMPORTANTE :O comando djnz decrementa e depois faz o teste, então se coloca o numero de
                        34
                        35     pulsoPWM:
001D D2A0               36             setb    p2.0            ;inicia pulso em "1"
001F EF                 37             mov     a,r7
0020 8EF0               38             mov     b,r6
0022 78F9               39             mov     r0,#0f9h        ;0.5MS natural ativo
0024 D8FE               40             djnz    r0,$            ;
                        41     aux1:
0026 D5E003             42             djnz    acc,chamaDelay1
0029 020031             43             jmp     aux_1
                        44     chamadelay1:
002C 120075             45             call    delay1Ms
002F 80F5               46             jmp     aux1
                        47     aux_1:
0031 D5F003             48             djnz    b,completa1
0034 02003C             49             jmp     zera
                        50     completa1:
0037 12007E             51             call    delay0_056Ms
003A 80F5               52             jmp     aux_1
                        53
                        54     zera:
003C C2A0               55             clr     p2.0            ;inicia pulso em "0"
003E 78F9               56             mov     r0,#0f9h        ;0.5MS natural desligado
0040 D8FE               57             djnz    r0,$            ;
0042 ED                 58             mov     a,r5
0043 8CF0               59             mov     b,r4
                        60     aux2:
0045 D5E003             61             djnz    acc,chamadelay12
0048 020050             62             jmp     aux_2
                        63     chamadelay12:
004B 120075             64             call    delay1Ms
004E 80F5               65             jmp     aux2
                        66     aux_2:
0050 D5F01D             67             djnz    b,completa2
0053 0B                 68             inc     r3
0054 BB1307             69             cjne    r3,#13h,auxPWM          ;pré-set antes da proxima onda que vai ter o find
0057 120083             70             call    encontraNovoNumero
005A 7B00               71             mov     r3,#00h
005C 80BF               72             jmp     pulsoPWM
                        73     auxPWM:                                 ;compensa o tempo de find
005E BB12BC             74             cjne    r3,#12h,pulsoPWM
0061 C3                 75             clr     c
0062 EC                 76             mov     a,r4                    ;
0063 9404               77             subb    a,#04h                  ;tira 2ms aproximadamente
0065 4007               78             jc      auxPWM1                 ; tem tempos em que não vai poder fazer uma subtração
0067 B40002             79             cjne    a,#00h,soPassa
006A 05E0               80             inc     acc
                        81     soPassa:
006C ACE0               82             mov     r4,acc          ;compensa o find
                        83     auxPWM1:
006E 80AD               84             jmp     pulsoPWM
                        85
                        86     completa2:
0070 12007E             87             call    delay0_056Ms
0073 80DB               88             jmp     aux_2
                        89
                        90     delay1Ms:                                               ;1MS= 1000uc
                        91
0075 78F7               92             Mov     R0,#0f7h                        ;1000uc totais de ciclos de maquina
0077 79F8               93             Mov     R1,#0f8h
0079 D8FE               94             djnz    R0,$
007B D9FE               95             djnz    R1,$
007D 22                 96             ret
                        97
                        98     delay0_056Ms:                                   ;subtrai dois alem do total por causa do jmp no compl
007E 7818               99             mov     r0,#18h
0080 D8FE              100             djnz    r0,$
0082 22                101             ret
                       102
