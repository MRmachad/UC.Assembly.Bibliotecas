                       427     functionset:
                       428
02CC C293              429             clr RS
                       430
02CE C297              431             CLR  DB7                ; |
02D0 C296              432             CLR  DB6                ; |
02D2 D295              433             SETB DB5                ; | DL=0~ datalength 4bits, se com 4 ou 8 bits de dados
02D4 C294              434             clr  DB4                ; | nible superio
                       435
02D6 D292              436             SETB E                  ; |
02D8 C292              437             CLR E                   ; | borda negativa no E
                       438
02DA 12035B            439             CALL delay
                       440
02DD D292              441             SETB E                  ; |
02DF C292              442             CLR E                   ; |borda negativa no E
                       443                                     ; |confirma a operação
                       444
02E1 D297              445             SETB DB7                ; | nibble inferior set N=1~2 linhas /  F=0~fonte 5 × 8 dots
                       446
02E3 D292              447             SETB E                  ; |
02E5 C292              448             CLR E                   ; | borda negativa no E
                       449                                     ; |function set low nibble sent
                       450
02E7 12035B            451             CALL delay              ; wait for BF to clear
                       452
02EA 22                453             ret
                       454
                       455     entrymodeset:
                       456
                       457     ; set to increment with shift
                       458
02EB C297              459             CLR DB7            ; |
02ED C296              460             CLR DB6            ; |
02EF C295              461             CLR DB5            ; |
02F1 C294              462             CLR DB4            ; | nible superior
                       463
02F3 D292              464             SETB E             ; |
02F5 C292              465             CLR E              ; | borda negativa no E
                       466
02F7 C297              467             CLR  DB7           ; |
02F9 D296              468             SETB DB6           ; |
02FB D295              469             SETB DB5           ; |I/D=1~Incremento (direção de movimento do cursor)
02FD C294              470             clr  DB4           ; |S=1~display  nao acompanha shift
                       471                                ; |nible inferior
                       472
02FF D292              473             SETB E             ; |
0301 C292              474             CLR E              ; | negative edge on E
                       475
0303 12035B            476             CALL delay         ; wait for BF to clear
                       477
0306 22                478             RET
                       479
                       480
                       481     displayonoffcontrol:
                       482
                       483     ; the display is turned on, the cursor is turned on and blinking is turned on
0307 C297              484             CLR DB7            ; |
0309 C296              485             CLR DB6            ; |
030B C295              486             CLR DB5            ; |
030D C294              487             CLR DB4            ; | nible superior
                       488
030F D292              489             SETB E             ; |
0311 C292              490             CLR E              ; | negative edge on E
                       491
0313 D297              492             SETB DB7           ; |
0315 D296              493             SETB DB6           ; |
0317 D295              494             SETB DB5           ; |
0319 D294              495             SETB DB4           ; | low nibble set
                       496
031B D292              497             SETB E             ; |
031D C292              498             CLR E              ; | negative edge on E
                       499
031F 12035B            500             CALL delay         ; |wait for BF to clear
                       501
0322 22                502             RET
                       503
