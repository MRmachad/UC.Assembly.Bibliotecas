                       307     StartPlay:
0207 D2A7              308             setb    trig
0209 00                309             nop
020A 00                310             nop
020B 00                311             nop
020C 00                312             nop
020D 00                313             nop
020E 00                314             nop
020F 00                315             nop
0210 00                316             nop
0211 00                317             nop
0212 00                318             nop
0213 C2A7              319             clr     trig
0215 30B2FD            320             jnb     echo,$                  ;
0218 20B2FD            321             jb      echo,$                  ;espera todo o pulso do echo
021B 7F00              322             mov     r7,#00h                 ;fica com um total momentaneo
021D 7D00              323             mov     r5,#00h                 ;fica com o resto momentaneo
                       324     operando:
021F 75F03A            325             mov     B,#3Ah                  ;B = 58 decimal
0222 E58A              326             mov     a,tl0                   ;pega o byte inferior do timer
0224 84                327             div     ab                      ;divide por 58, a recebe a parte inteira e b o resto
0225 2F                328             add     a,r7                    ;a = a + r7(parte inteira ja divida)
0226 FF                329             mov     r7,a                    ;adiciona o valor de divisão a r7(acumula todo a parte inteir
0227 E5F0              330             mov     a,b
0229 2D                331             add     a,r5
022A FD                332             mov     r5,a                    ;acumula o resto em r5
022B AE8C              333             mov     r6,th0
022D BE0021            334             CJNE    r6,#00h,recarrega       ;Se th nao for 0 ele vai para recarrega
0230 75F03A            335             mov     b,#3Ah                  ;B = 58 decimal
0233 ED                336             mov     a,r5                    ;a pega o byte resto total
0234 84                337             div     ab                      ;divide por 58, a recebe a parte inteira e b o resto
0235 2F                338             add     a,r7                    ;a = a + r7(parte inteira ja divida)
0236 B40003            339             CJNE    a,#00h,verifica_dois
0239 020244            340             jmp     retoma
                       341     verifica_dois:
023C B40103            342             CJNE    a,#01h,reforma
023F 020244            343             jmp     retoma
                       344     reforma:
0242 9402              345             subb    a,#02h                  ;ajusta ao erro do sensor de 2 cm
                       346     retoma:
0244 A2E2              347             mov     c,acc.2
0246 92B7              348             mov     p3.7,c
0248 A2E1              349             mov     c,acc.1
024A 92B6              350             mov     p3.6,c
024C A2E0              351             mov     c,acc.0
024E 92B5              352             mov     p3.5,c
0250 22                353             ret
                       354     recarrega:
0251 758AFF            355             mov     tl0,#0ffh               ;recarrega
0254 D58CC8            356             djnz    TH0,operando            ;decrementa th0
0257 80C6              357             jmp     operando                ; para o caso de o valor de th0 ter o valor 1 ira decrementar
                       358
